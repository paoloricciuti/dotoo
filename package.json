{
  "name": "dotoo",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "svelte": "dist/index.js",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui-light.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@smui/button": "^6.0.0-beta.9",
    "@smui/dialog": "^6.0.0-beta.9",
    "@smui/fab": "^6.0.0-beta.7",
    "@smui/icon-button": "^6.0.0-beta.9",
    "@smui/textfield": "^6.0.0-beta.9",
    "@smui/top-app-bar": "^6.0.0-beta.8",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "^1.0.0-next.36",
    "@sveltejs/kit": "next",
    "smui-theme": "^6.0.0-beta.7",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module"
}
